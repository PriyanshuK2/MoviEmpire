{
    "version": 2,
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "framework": "create-react-app",
    "headers": [
        {
          "source": "/api/(.*)",
          "headers": [
            { "key": "Access-Control-Allow-Origin", "value": "*" },
            { "key": "Access-Control-Allow-Methods", "value": "GET,OPTIONS" }
          ]
        }
      ]
  }
